@model Laundry.Models.DetailPelayanan

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>Data Detail Pelayanan</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdKaryawan" />
            <div class="form-group">
                <label asp-for="IdPesanan" class="control-label"></label>
                <input asp-for="IdPesanan" class="form-control" />
                <span asp-validation-for="IdPesanan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Simpan" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Kembali ke Daftar Detail Pelayanan</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
